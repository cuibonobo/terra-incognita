{
  "name": "terra-incognita-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run cli -- build",
    "clean": "rimraf ./dist",
    "cli": "node --loader ts-node/esm --experimental-vm-modules --es-module-specifier-resolution=node --no-warnings ./src/cli.ts",
    "publish": "wrangler publish",
    "scrape": "npm run cli -- scrape landscape",
    "start": "miniflare --live-reload --debug",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuibonobo/terra-incognita.git"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "@cloudflare/wrangler": "^1.19.8",
    "@types/node": "^16.11.10",
    "commander": "^9.0.0",
    "dotenv": "^15.0.0",
    "esbuild": "^0.14.15",
    "miniflare": "^2.2.0",
    "node-fetch": "^3.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "itty-router": "^2.4.10",
    "scraper-instagram": "^1.0.17"
  }
}
