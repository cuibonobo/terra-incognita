{
  "name": "terra-incognita",
  "version": "1.0.0",
  "description": "A sort of luminous, geometric, incandescent immensity.",
  "type": "module",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production parcel build src/index.html",
    "build:worker": "node build.js",
    "clean": "rimraf ./dist",
    "clean:cache": "rimraf ./.parcel-cache",
    "clean:all": "npm run clean && npm run clean:cache",
    "cli": "node --loader ts-node/esm --experimental-modules --es-module-specifier-resolution=node --no-warnings ./src/cli.ts",
    "copy-assets": "ncp ./assets ./dist/assets",
    "mkdir:assets": "mkdirp ./dist/assets",
    "scrape": "npm run cli -- scrape landscape",
    "serve": "http-server -a localhost -p 8000 ./dist",
    "start": "npm run clean && npm run mkdir:assets && npm run copy-assets && npm run start:js",
    "start:js": "parcel src/index.html --port 8000",
    "start:worker": "miniflare --live-reload --debug",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuibonobo/terra-incognita.git"
  },
  "keywords": [
    "art",
    "philosophy",
    "landscape"
  ],
  "author": "Jen Garcia <jen@haverstack.com> (https://haverstack.com/)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/cuibonobo/terra-incognita/issues"
  },
  "homepage": "https://github.com/cuibonobo/terra-incognita#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "@types/node": "^16.11.10",
    "commander": "^9.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^15.0.0",
    "esbuild": "^0.14.15",
    "http-server": "^14.0.0",
    "miniflare": "^2.2.0",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "node-fetch": "^3.2.0",
    "parcel": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "image-process": "^4.1.1",
    "preact": "^10.6.1",
    "react-router-dom": "^6.0.2",
    "scraper-instagram": "^1.0.17"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  }
}
